@const = internal constant i128 30
@a = internal global i128 0

define internal void @method() {
  call void @exit(i128 10)
  ret void
}

define internal void @lib..init() {
  ret void
}

declare void @exit(i128 signext)

define internal void @import-slot..init() {
  %1 = load i128* @const
  store i128 %1, i128* @a
